{
    "env_name": "HopperBulletEnv-v0",
    "wrappers": [
        [
            "DiscretizedObservationWrapper",
            {
                 "n_bins": 8,
                 "low": [-2.4, -2.0, -0.42, -3.5],
                 "high": [2.4, 2.0, 0.42, 3.5]
            }],
         [
             "DiscretizedActionWrapper",
            {
                 "n_bins": 8,
                 "low": [-1.0, -1.0, -1.0, -1.0],
                 "high": [1.0, 1.0, 1.0, 1.0]
            }]
     ],
    "policy_name": "DqnPolicy",
    "policy_params": {
        "batch_size": 32,
        "layer_sizes": [32, 32],
        "double_q": true,
        "dueling": true,
        "model_type": "dense",
        "step_size": 1
    },
    "train_params": {
        "lr": 0.001,
        "epsilon": 1.0,
        "epsilon_final": 0.02,
        "warmup_episodes": 450,
        "log_every_episode": 10,
        "n_episodes": 500,
        "target_update_every_step": 10
    }
}